{
  "name": "@encoradxp/mat-lib-test",
  "version": "0.2.32",
  "publicConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && node post-build.js",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "preinstall": "npm install vite-plugin-vuetify",
    "postinstall": "cp -r ./src/plugins ../../../src && cp -r ./src/styles ../../../src"
  },
  "main": "./dist/mat-lib-test.umd.js",
  "module": "./dist/mat-lib-test.es.js",
  "files": [
    "dist",
    "src/assets"
  ],
  "dependencies": {
    "@fontsource/lato": "^5.0.15",
    "@fortawesome/fontawesome-pro": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@material/web": "^1.0.0",
    "@previewjs/cli": "^1.27.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-meta": "^2.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.4.9"
  },
  "exports": {
    ".": {
      "import": "./dist/mat-lib-test.es.js",
      "require": "./dist/mat-lib-test.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  }
}
